Cell["01 - Kottler Global Foliation: Crossing vs Asymptotic Stall", "Title",ExpressionUUID->"11cfe22f-90af-4c90-a657-5fc7940e8161"],

Cell["\<\
Operational test for question 2: in Schwarzschild-de Sitter, does a regular \
global foliation Theta cross horizons or stall at them?\
\>", "Text",ExpressionUUID->"ae03c992-cfd5-4d5e-af3c-c3adc1173fe2"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"r_", ",", "M_", ",", "H_"}], "]"}], ":=", 
   RowBox[{"1", "-", 
    FractionBox[
     RowBox[{"2", " ", "M"}], "r"], "-", 
    RowBox[{
     SuperscriptBox["H", "2"], " ", 
     SuperscriptBox["r", "2"]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"c0eb5a4a-41b2-42f8-aaa5-e7d03a4995df"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Theta", "\[Equal]", "T", "\[Equal]", 
    RowBox[{"t", "+", 
     RowBox[{"g", "[", "r", "]"}]}]}], "&&", 
   RowBox[{
    RowBox[{
     SuperscriptBox["g", "\[Prime]",
      MultilineFunction->None], "[", "r", "]"}], "\[Equal]", 
    FractionBox[
     SqrtBox[
      RowBox[{"1", "-", 
       RowBox[{"f", "[", 
        RowBox[{"r", ",", "M", ",", "H"}], "]"}]}]], 
     RowBox[{"f", "[", 
      RowBox[{"r", ",", "M", ",", "H"}], "]"}]]}]}],
  HoldForm]], "Input",ExpressionUUID->"60f8759d-58cb-4aac-a496-4d3b2ecab199"],

Cell[BoxData[
 TagBox[
  RowBox[{"XTheta", "\[Equal]", 
   RowBox[{"-", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"gInv", "[", 
       RowBox[{"mu", ",", "nu"}], "]"}], " ", 
      RowBox[{
       SubscriptBox["\[PartialD]", 
        RowBox[{"x", "[", "mu", "]"}]], "Theta"}], " ", 
      RowBox[{
       SubscriptBox["\[PartialD]", 
        RowBox[{"x", "[", "nu", "]"}]], "Theta"}]}], ")"}]}], "\[Equal]", 
   "1"}],
  HoldForm]], "Input",ExpressionUUID->"72b1189b-321c-4cb1-8ea7-faaff4229c1a"],

Cell[BoxData[
 TagBox[
  RowBox[{"ds2TR", "\[Equal]", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "f"}], " ", 
     SuperscriptBox["dT", "2"]}], "+", 
    RowBox[{"2", " ", 
