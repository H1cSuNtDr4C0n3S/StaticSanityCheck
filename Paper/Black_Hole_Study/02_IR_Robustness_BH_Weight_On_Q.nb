Notebook[{
Cell["02 - IR Robustness: BH Weight on Q=I1/I0", "Title",ExpressionUUID->"69748d6a-1810-4bf5-a417-5b44f6e00cdc"],

Cell["\<\
Toy test: de Sitter background plus local BH defect. Goal: quantify when \
deltaQ stays volume-suppressed and when non-integrable core behavior can make \
it O(1).\
\>", "Text",ExpressionUUID->"65252316-4e7c-40b8-8f55-7f1cccccf75b"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Q", "\[Equal]", 
    FractionBox["I1", "I0"]}], "&&", 
   RowBox[{"I1", "\[Equal]", 
    RowBox[{"I1bg", "+", "deltaI1BH"}]}], "&&", 
   RowBox[{"I0", "\[Equal]", "I0cosmo"}]}],
  HoldForm]], "Input",ExpressionUUID->"00972bc2-3415-4157-8dd4-863f4549b8ac"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"deltaI1BH", "[", "p", "]"}], "\[Equal]", 
   RowBox[{"4", " ", "\[Pi]", " ", "a", " ", "K2bg", " ", 
    SuperscriptBox["rs", "p"], " ", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "reps", "rMatch"], 
     RowBox[{
      SuperscriptBox["r", 
       RowBox[{"2", "-", "p"}]], 
      RowBox[{"\[DifferentialD]", "r"}]}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"d6bc06b1-dc8f-4309-93f8-6f837dc40a2a"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"deltaQ", "\[Equal]", 
    FractionBox["deltaI1BH", "I0"]}], "&&", 
   RowBox[{"I0", "\[Equal]", 
    RowBox[{
     FractionBox["1", "3"], " ", 
     RowBox[{"(", 
      RowBox[{"4", " ", "\[Pi]"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["Rc", "3"], "-", 
       SuperscriptBox["reps", "3"]}], ")"}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"ae1f9218-ec57-4f3d-8db0-894b51c4c704"],

Cell[BoxData[
 RowBox[{"HoldForm", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"p", "<", "3"}], "\[Rule]", 
    RowBox[{"integrable", " ", 
     RowBox[{"(", 
      RowBox[{"finite", " ", "core", " ", "contribution"}], ")"}]}]}], ",", 
   RowBox[{
    RowBox[{"p", ">", "3"}], "\[Rule]", 
    RowBox[{"non", "-", 
     RowBox[{"integrable", " ", "core", " ", "sensitivity"}]}]}]}], 
  "]"}]], "Input",ExpressionUUID->"e2ca9e0a-cd1f-4908-8b88-150cfca9246b"]
},
