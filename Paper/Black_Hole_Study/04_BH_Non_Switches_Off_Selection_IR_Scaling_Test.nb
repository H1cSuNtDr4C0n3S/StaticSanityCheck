Notebook[{
Cell["04 - BH Non-Switches-Off Selection (IR Scaling Test)", "Title",ExpressionUUID->"78d60532-888b-4371-b185-646c3aa01f31"],

Cell["\<\
Objective: validate that BH contribution to Q is IR-suppressed and decreases \
as cosmic domain grows (H -> H/2).\
\>", "Text",ExpressionUUID->"13ddf925-b249-4432-8b16-eea18efc6cfc"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"DTheta", "[", 
    RowBox[{"M_", ",", "H_", ",", "z2_"}], "]"}], "\[Equal]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"r", ">", "0"}], "|", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"r", ",", "M", ",", "H"}], "]"}], "\[GreaterEqual]", "z2"}]}], 
    "}"}]}],
  HoldForm]], "Input",ExpressionUUID->"4d7957bc-2963-46d7-89cc-72edd93c2a9d"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Q", "[", 
    RowBox[{"M_", ",", "H_"}], "]"}], "\[Equal]", 
   FractionBox[
    RowBox[{"I1", "[", 
     RowBox[{"M", ",", "H"}], "]"}], 
    RowBox[{"I0", "[", 
     RowBox[{"M", ",", "H"}], "]"}]]}],
  HoldForm]], "Input",ExpressionUUID->"4c6e8c76-6e27-48b1-9f1b-fd06b9b858ad"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"deltaQ", "[", "H_", "]"}], "\[Equal]", 
   RowBox[{
    RowBox[{"Q", "[", 
     RowBox[{"M", ",", "H"}], "]"}], "-", 
    RowBox[{"Q", "[", 
     RowBox[{"0", ",", "H"}], "]"}]}]}],
  HoldForm]], "Input",ExpressionUUID->"45d2e6f7-dd18-483c-823a-1d687c1fd25e"],

Cell[BoxData[
 TagBox[
  RowBox[{"testIRRatio", "\[Equal]", 
   FractionBox[
    RowBox[{"deltaQ", "[", 
     FractionBox["H", "2"], "]"}], 
    RowBox[{"deltaQ", "[", "H", "]"}]]}],
  HoldForm]], "Input",ExpressionUUID->"067b268b-8ca4-4351-ac71-62e1f1d91a5d"]
},
